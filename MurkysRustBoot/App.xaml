<Application x:Class="MurkysRustBoot.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MurkysRustBoot"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style_Colors.xaml"/>
                <ResourceDictionary Source="Style_Controls.xaml"/>
                <ResourceDictionary Source="ControlTemplates.xaml"/>
                <ResourceDictionary Source="GuiText.xaml"/>
                <ResourceDictionary Source="DataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
